    retries = Retry(total=5, backoff_factor=2, status_forcelist=[500, 502, 503, 504], allowed_methods=["POST"])
    session.mount("https://", HTTPAdapter(max_retries=retries))
    
    try:
        response = session.post(URL, headers=headers, json=data, timeout=15)  # Âª∂ÈïøË∂ÖÊó∂Êó∂Èó¥
        response.raise_for_status()
        success_count += 1
        print(f"üéâüåü [SUCCESS] ‚úÖ ÊàêÂäü {success_count} Ê¨° üåàüéä")
        return "SUCCESS"
    except requests.exceptions.RequestException as e:
        failure_count += 1
        print(f"üòûüí• [FAILURE] ‚ùå Â§±Ë¥• {failure_count} Ê¨° ‚ö†Ô∏èüò£")
        if hasattr(e.response, 'status_code'):
            if e.response.status_code == 401:
                print(f"[ERROR] üåê API {api_index} ËØ∑Ê±ÇÂ§±Ë¥• | Áä∂ÊÄÅ: 401 | APIËÆ§ËØÅÂ§±Ë¥• üö´")
                return "FAILURE"  # ‰∏çÂÅúÁî®API
            elif e.response.status_code == 402:
                print(f"[ERROR] üí∏ API {api_index} ËØ∑Ê±ÇÂ§±Ë¥• | Áä∂ÊÄÅ: 402 | APIÈ¢ùÂ∫¶‰∏çË∂≥ÔºåÈúÄË¶ÅÊîØ‰ªò üö´")
                return "FAILURE"  # ‰∏çÂÅúÁî®API
            elif e.response.status_code == 404:
                print(f"[ERROR] üïí API {api_index} ËØ∑Ê±ÇÂ§±Ë¥• | Áä∂ÊÄÅ: 404 | Á´ØÁÇπ {URL} ÂèØËÉΩÊó†Êïà üö´")
                return "FAILURE"  # ‰∏çÂÅúÁî®API
        return "FAILURE"

def auto_chat_concurrent():
    global success_count, failure_count, start_time, last_stats_time
    print("üåç ÂºÄÂßãÂä®ÊÄÅÁ∫øÁ®ãÂπ∂ÂèëËÅäÂ§© - Êåâ Ctrl+C ÈÄÄÂá∫ ‚ú®")
    print(f"üìä ÊÄªÈóÆÈ¢òÊï∞: {len(QUESTIONS)}, ÊÄªAPIÊï∞: {len(API_KEYS)} üåü")
    
    active_api_keys = API_KEYS.copy()  # ‰ΩøÁî®ÊâÄÊúâAPIÔºå‰∏çÁßªÈô§
    
    while True:
        try:
            if len(active_api_keys) < 30:
                print(f"‚ö†Ô∏è Ë≠¶Âëä: ÂèØÁî®APIÊï∞Èáè ({len(active_api_keys)}) Â∞è‰∫éÁ∫øÁ®ãÊï∞ (30) üö®")
            
            # 
            batch_questions = random.sample(QUESTIONS, min(30, len(QUESTIONS)))
            batch_api_keys = random.sample(active_api_keys, min(30, len(active_api_keys)))
            
            # 
            with ThreadPoolExecutor(max_workers=30) as executor:
                futures = [
                    executor.submit(chat_with_gaia, api_key, API_KEYS.index(api_key), q, i+1)
                    for i, (api_key, q) in enumerate(zip(batch_api_keys, batch_questions))
                ]
                
                # 
                for future in futures:
                    future.result()
            
            # 
            current_time = time.time()
            elapsed_time = current_time - last_stats_time
            if elapsed_time >= stats_interval:
                total_minutes = int((current_time - start_time) // 60) + 1
                print(f"\nüìà Á¨¨ {total_minutes} ÂàÜÈíüÁªüËÆ°: üéâ‚úÖ ÊàêÂäü {success_count} Ê¨° üåü, üòû‚ùå Â§±Ë¥• {failure_count} Ê¨° ‚ö†Ô∏è\n")
                last_stats_time = current_time
            
            # 
            if not active_api_keys:
                print("üö´ ÊâÄÊúâAPIÂùá‰∏çÂèØÁî®ÔºåÁ®ãÂ∫èÈÄÄÂá∫ üí§")
                break
            
        except KeyboardInterrupt:
            print(f"\nüé¨ ÈÄÄÂá∫Á®ãÂ∫èÔºåÊÄªÊàêÂäüÊ¨°Êï∞: üéâ‚úÖ {success_count} Ê¨° üåü, ÊÄªÂ§±Ë¥•Ê¨°Êï∞: üòû‚ùå {failure_count} Ê¨° ‚ö†Ô∏è")
            break
        except Exception as e:
            print(f"üõë ‰∏ªÁ∫øÁ®ãÈîôËØØ: {e} üö®")
            time.sleep(5)

if __name__ == "__main__":
    if not API_KEYS or not QUESTIONS:
        print("‚ùå ÈîôËØØ: ËØ∑Âú® API_KEYS Âíå QUESTIONS ‰∏≠Â°´ÂÖ•Êï∞ÊçÆ üö´")
    else:
